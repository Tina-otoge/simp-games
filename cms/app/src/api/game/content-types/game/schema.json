{
  "kind": "collectionType",
  "collectionName": "games",
  "info": {
    "singularName": "game",
    "pluralName": "games",
    "displayName": "Game",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "comment": {
      "type": "richtext"
    },
    "medias": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "links": {
      "type": "richtext"
    },
    "release_date": {
      "type": "string"
    },
    "acquired_date": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::session.session",
      "mappedBy": "game"
    },
    "ero": {
      "type": "boolean",
      "default": false
    }
  }
}
